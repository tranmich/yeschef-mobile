{
  "expo": {
    "name": "YesChef",
    "slug": "yeschef-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "yeschef",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "fonts": [
      "./assets/images/fonts/Nunito-Regular.ttf",
      "./assets/images/fonts/Nunito-Light.ttf",
      "./assets/images/fonts/Nunito-ExtraBold.ttf"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yeschef.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yeschef.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableMinifyInReleaseBuilds": true
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-secure-store",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "1ac57b40-1ae9-4cf5-9adb-8ca4eed47fde"
      }
    }
  }
}
